/var/log/apache2/sru/access_log {
	compress
	dateext
	maxage 365
	rotate 12
	size=+1024k
	notifempty
	missingok
	prerotate
		/etc/init.d/apache2 check-reload
	endscript
	postrotate
		/etc/init.d/apache2 reload
	endscript
}

/var/log/apache2/sru/error_log {
	compress
	dateext
	maxage 365
	rotate 12
	size=+1024k
	notifempty
	missingok
	prerotate
		/etc/init.d/apache2 check-reload
	endscript
	postrotate
		/etc/init.d/apache2 reload
	endscript
}
